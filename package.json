    "gatsby-plugin-netlify": "1.0.19",
    "gatsby-plugin-offline": "1.0.15",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-styled-components": "2.0.11",
    "gatsby-plugin-typography": "1.7.18",
    "gatsby-source-graphcms": "0.0.1",
